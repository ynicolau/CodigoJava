programa {
    
    inclua biblioteca Matematica-->m
    
	funcao inicio() {
	    
	    inteiro x1
	    inteiro y1
	    inteiro x2
	    inteiro y2
	    real d
	    
	    escreva("Insira x1: ")
	    leia(x1)
	    
	    escreva("Insira y1: ")
	    leia(y1)
	    
	    escreva("Insira x2: ")
	    leia(x2)
	    
	    escreva("Insira y2: ")
	    leia(y2)
	   
	    d = m.raiz(((x2 - x1)^2 + (y2-y1)^2) , 2)
	    
	    escreva("Resultado: ", d)
	    
	    
	    
	    
	    
	    
		
	}
}
